{
  "name": "usx-starter-1-0-0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=18.18 <23"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "type:check": "tsc --noEmit",
    "format": "prettier --write .",
    "bundle:analyze": "ANALYZE=true next build",
    "seed:owner": "bash ./scripts/seed-owner.sh",
    "check:emoji": "bash -lc \"! grep -R --exclude-dir=node_modules --exclude-dir=.git -n -- $'[\\U0001F300-\\U0001FAFF]' app src\""
  },
  "dependencies": {
    "@supabase/ssr": "0.5.2",
    "@supabase/supabase-js": "2.54.0",
